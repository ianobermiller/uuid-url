{
  "name": "uuid-url",
  "version": "0.0.0-development",
  "description": "Convert a UUID to a shorter, URL-safe base64 string and back.",
  "repository": "github:ianobermiller/uuid-url",
  "keywords": [
    "uuid",
    "base64",
    "url-safe",
    "url",
    "browser"
  ],
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "author": "Ian Obermiller <ian@obermillers.com>",
  "license": "ISC",
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=14.16"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/uuid": "^8.3.4",
    "esbuild": "^0.14.38",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "tsm": "^2.2.1",
    "typescript": "^4.6.4",
    "uvu": "^0.5.3"
  },
  "scripts": {
    "build:esm": "esbuild --outfile=dist/esm/index.js --format=esm --bundle src/index.ts && tsc",
    "build:cjs": "esbuild --outfile=dist/cjs/index.cjs --format=cjs --bundle src/index.ts && tsc -p tsconfig-cjs.json",
    "build": "rimraf dist && yarn build:esm && yarn build:cjs",
    "test": "tsm src/test.ts"
  },
  "files": [
    "dist/cjs/index.*",
    "dist/esm/index.*"
  ]
}
